
NODE_ENV=production
PORT=3000


REDIS_URL=redis://redis:6379
=
MINIO_ENDPOINT=minio
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123


POSTGRES_URL=postgresql://bigdata:bigdata123@postgres:5432/bigdata_processor
POSTGRES_USER=bigdata
POSTGRES_PASSWORD=bigdata123
POSTGRES_DB=bigdata_processor

OLLAMA_URL=http://ollama:11434
OLLAMA_MODEL=llava:latest


# Set to true for worker nodes, false for web servers
WORKER_MODE=false

# Worker ID (1-4 for worker nodes)
WORKER_ID=1

# Assigned partitions (comma-separated, 0-7)
# Worker 1: 0,1
# Worker 2: 2,3
# Worker 3: 4,5
# Worker 4: 6,7
PARTITIONS=0,1


PROMETHEUS_URL=http://prometheus:9090
GRAFANA_URL=http://grafana:3000
GRAFANA_ADMIN_PASSWORD=admin123


# Number of parallel workers per node
WORKER_POOL_SIZE=4

# Batch size for bulk ingestion
BATCH_SIZE=100

# Queue processing concurrency
QUEUE_CONCURRENCY=4

# Maximum file upload size (bytes)
MAX_FILE_SIZE=10485760

# Request timeout (milliseconds)
REQUEST_TIMEOUT=300000


# JWT secret for authentication (generate with: openssl rand -base64 32)
JWT_SECRET=your-secret-key-here

# CORS allowed origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost

# API rate limit (requests per minute)
RATE_LIMIT=100


LOG_LEVEL=info
LOG_FORMAT=json


ENABLE_METRICS=true
ENABLE_TRACING=false
ENABLE_CACHING=true


# ImageNet API
IMAGENET_API_URL=http://www.image-net.org

# Unsplash API (for dataset download)
UNSPLASH_ACCESS_KEY=your-unsplash-key-here

